{
  "name": "invoker-gatsby",
  "private": true,
  "description": "Meridian.id official starter for gatsby website project",
  "version": "0.1.0",
  "author": "Ongki 'vasilenka' Herlambang <ongki@herlambang.design | o@meridian.id>",
  "scripts": {
    "build": "gatsby build",
    "static": "gatsby build --prefix-paths",
    "dev": "gatsby develop",
    "local": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "clean": "gatsby clean",
    "cleanDev": "gatsby clean && gatsby develop",
    "cleanBuild": "gatsby clean && gatsby build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.27",
    "@mdx-js/react": "^1.0.27",
    "@mdx-js/tag": "^0.20.3",
    "classnames": "^2.2.6",
    "date-fns": "^2.2.1",
    "external-load": "^1.0.0",
    "framer-motion": "^1.6.7",
    "gatsby": "^2.15.16",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-google-analytics": "^2.1.16",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-gtag": "^1.0.11",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-mdx": "^1.0.42",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-offline": "^3.0.7",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.14",
    "gatsby-plugin-sharp": "^2.2.22",
    "gatsby-plugin-sitemap": "^2.2.12",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-source-prismic-graphql": "^3.3.1",
    "gatsby-transformer-sharp": "^2.1.17",
    "glamor": "^2.20.40",
    "invoker-base": "^1.0.6",
    "invoker-layout": "^1.0.1",
    "invoker-tokens": "^1.0.13",
    "lodash": "^4.17.11",
    "mapbox-gl": "^1.9.0",
    "node-sass": "^4.11.0",
    "prism-react-renderer": "^0.1.7",
    "prismic-reactjs": "^1.0.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-focus-lock": "^2.2.1",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.9.0",
    "react-intersection-observer": "^8.26.1",
    "react-loadable": "^5.5.0",
    "react-mapbox-gl": "^4.8.3",
    "react-remove-scroll": "^2.2.0",
    "recompose": "^0.30.0",
    "use-persisted-state": "^0.3.0"
  },
  "devDependencies": {
    "eslint-config-react-app": "^4.0.1",
    "gh-pages": "^2.0.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/meridianid/invoker-gatsby"
